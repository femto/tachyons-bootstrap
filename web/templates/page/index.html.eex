  <div class="ba br2 b--black-10 shadow-1 pa3 mv3">
    <p class="mt0 f6 fw6 silver">EXAMPLE</p>
    <select multiple class="w-100 br2 b--black-20 ph3 pv1 f6">
      <option>1</option>
      <option>2</option>
      <option>3</option>
      <option>4</option>
      <option>5</option>
    </select>
  </div>

  <h3>Static control</h3>
  <div class="ba br2 b--black-10 shadow-1 pa3 mv3">
